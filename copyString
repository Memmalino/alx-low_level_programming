char *_strcpy(char *dest, char *src){
    int i, j = 0, m;
 while (src[j] != '\0')
 {
     j++;
 }
 for ( i = 0; i < j; i++)
 {
      dest[i] = src[i];
    //   printf("%c", dest[i]);
 }
 dest[j]='\0';
 return (dest);
//   printf("%c", dest[i]);  
}
